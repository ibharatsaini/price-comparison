                html,
                body {
                    height: 100%;
                    width: 100%;
                    margin: 0%;
                }
        
                #load {
                    width: 100%;
                    height: 100%;
                    position: absolute;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    color: rgb(92, 86, 30);
                    background-color: rgba(0, 0, 0, 0.0);
                    letter-spacing: 6px;
                    word-spacing: -7px;
                }
        
                .heck1 {
                    font-size: 40px;
                    /* text-transform: lowercase; */
                    animation-name: boom;
                    animation-duration: 2s;
                    animation-timing-function: linear;
                    animation-delay: 0s;
                    animation-iteration-count: infinite;
                    margin-right: 10px;
                    font-weight: 100;
                    transform: scale(0);
                    margin: 0;
                    font-weight: 300;
                }
        
                #h1 {
                    animation-delay: 0.10s;
                }
        
                #h2 {
                    animation-delay: 0.15s;
                }
        
                #h3 {
                    animation-delay: 0.25s;
                }
        
                #h4 {
                    animation-delay: 0.30s;
                }
        
                #h5 {
                    animation-delay: 0.35s;
                }
        
                #h6 {
                    animation-delay: 0.40s;
                }
        
                #h7 {
                    animation-delay: 0.45s;
                }
        
                #h8 {
                    animation-delay: 0.50s;
                }
        
                #h9 {
                    animation-delay: 0.55s;
                }
        
                #h10 {
                    animation-delay: 0.70s;
                }
        
                #h11 {
                    animation-delay: 0.75s;
                }
        
                #h12 {
                    animation-delay: 0.80s;
                }
        
                #h13 {
                    animation-delay: 0.85s;
                }
                
        
                @keyframes boom {
        
                    0%,
                    100% {
                        transform: scale(1.2);
                    }
        
                    90% {
                        transform: scale(0.8);
                    }
        
                    99% {
                        transform: scale(0);
                    }
        
                }
        
                #mainKamain {
                    display: flex;
                    height: 100%;
                    width: 100%;
                    /* position: absolute; */
                    justify-content: center;
                    align-items: center;
                }
        
                #mainKamain img {
                    max-width: 100%;
                    max-height: 100%;
                    zoom: 5;
                    position: absolute;
                    filter: blur(6px);
                }
