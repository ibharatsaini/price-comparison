                if (el.checked) {
                    if (idList.length == 0) {
                        categ = el.name
                        console.log(categ)
                    }
                    // console.log(el.id)
                    if (!(el.id in idList)) {
                        // console.log(el.name)
                        if (categ == el.name) {
                            if (idList.length < 3) {
                                idList.push(el.id)
                                // console.log(idList.length)
                            } else {
                                el.checked = false;
                                animatingMessage('Three Products Are Not Enough?', false)
                                return
                            }
                        } else {
                            el.checked = false
                            animatingMessage('You Can Compare Products Of Same Category', false)
                            return
                        }
                    }
                } else {
                    var newIndex = idList.indexOf(el.id)
                    // console.log(newIndex);
                    if (newIndex == idList.length - 1) {
                        idList.pop()
                        // console.log(idList)
                    } else {
                        idList.splice(newIndex, newIndex + 1)
                    }
