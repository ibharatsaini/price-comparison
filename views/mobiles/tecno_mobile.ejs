                if (idList.length > 0) {
                    divOfPopUp.style.display = "block"
                    spanOfItem.textContent = idList.length;
                    divOfPopUp.prepend(spanOfItem)
                } else {
                    divOfPopUp.style.display = "none"
                }
            }

