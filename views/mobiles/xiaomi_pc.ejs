            function ltp(t, clone) {
                // let y = response
                if (t) {
                    let n = clone.data.sort((y, x) => { return y.stores[0].price - x.stores[0].price })
                    return n
                } else {
                    return clone.data.sort((x, y) => { return y.stores[0].price - x.stores[0].price })
                }

